{
  "name": "react-starter-ts",
  "version": "0.0.1",
  "description": "quickstart boilerplate for react using typescript/webpack/babel.",
  "keywords": [
    "react",
    "components"
  ],
  "homepage": "https://examples-hub.github.io/react-starter-ts",
  "license": "MIT",
  "author": "jinyaoo <jinyaoo@qq.com>",
  "main": "src/index.ts",
  "scripts": {
    "// storybook": "echo 'UNFINISHED start-storybook -p 8999 -c .storybook'",
    "build": "cross-env REACT_APP_ENV=react NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build2": "npm run build:types && npm run build:js",
    "build:dev": "cross-env REACT_APP_ENV=react NODE_ENV=production webpack --config ./webpack/webpack.config.prod.js",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "build:ts": "babel src --out-dir lib --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf ./build/",
    "demo": "cross-env REACT_APP_ENV=reacthot webpack serve --hot --config ./webpack/webpack.config.demo.js",
    "dev": "",
    "formatter": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "formatter:check": "npm run formatter -- --check",
    "help": "echo 'exec npm start to view the demo'",
    "lint": "eslint src --ext .ts,.tsx --fix -o eslint-log.log",
    "start": "npm run demo",
    "test": "cross-env REACT_APP_ENV=react jest --config ./jest.config.js",
    "type-check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run formatter"
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@faker-js/faker": "^7.4.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.22.0",
    "eslint-config-alloy": "^4.6.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "global-jsdom": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^20.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "npm-force-resolutions": "^0.0.10",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "yargs": "^17.5.1"
  }
}
